{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import"
  ],
  "env": {
    "esm": {
      "presets": [["@babel/preset-env", { "modules": false }]],
      "plugins": [
        ["@babel/plugin-transform-runtime", { "useESModules": true }]
      ]
    },
    "production": {
      "plugins": [
        "transform-react-remove-prop-types"
      ]
    }
  }
}